{
  "name": "@devclub/site",
  "version": "2.0.0",
  "description": "Devclub site platform: devclub.eu & devclub.ee",
  "author": [
    "Anton Stalnuhhin <anton.stalnuhhin@gmail.com>"
  ],
  "private": true,
  "homepage": "https://github.com/devclub/site",
  "repository": {
    "type": "git",
    "url": "devclub/site.git"
  },
  "scripts": {
    "ng": "ng",
    "clean": "rimraf dist",
    "start:eu": "ng serve --aot --open",
    "start:ee": "ng serve --configuration=dev-ee --aot --open",
    "start": "npm run start:eu",
    "build:dev:eu": "npm run clean && ng build --dev --env=dev-eu",
    "build:dev:ee": "npm run clean && ng build --dev --env=dev-ee",
    "build:prod:eu": "npm run clean && ng build --prod --env=prod-eu",
    "build:prod:ee": "npm run clean && ng build --prod --env=prod-ee",
    "extract": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/*.json --sort --format json",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/common": "6.0.9",
    "@angular/compiler": "6.0.9",
    "@angular/core": "6.0.9",
    "@angular/forms": "6.0.9",
    "@angular/http": "6.0.9",
    "@angular/platform-browser": "6.0.9",
    "@angular/platform-browser-dynamic": "6.0.9",
    "@angular/router": "6.0.9",
    "@fortawesome/fontawesome": "1.1.8",
    "@fortawesome/fontawesome-free-brands": "5.0.13",
    "@fortawesome/fontawesome-free-regular": "5.0.13",
    "@fortawesome/fontawesome-free-solid": "5.0.13",
    "@ngx-translate/core": "10.0.2",
    "bootstrap": "4.1.2",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.7",
    "ngx-bootstrap": "3.0.1",
    "rxjs": "6.2.2",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.9",
    "@angular/language-service": "6.0.9",
    "@angular/tsc-wrapped": "5.0.0-beta.7",
    "@biesbjerg/ngx-translate-extract": "2.3.4",
    "@types/node": "10.5.2",
    "codelyzer": "4.4.2",
    "cpr": "3.0.1",
    "rimraf": "2.6.2",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "2.7.2",
    "@angular-devkit/build-angular": "~0.6.8"
  }
}
