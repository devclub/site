{
  "name": "@devclub/site",
  "version": "2.0.0",
  "description": "Devclub site platform: devclub.eu & devclub.ee",
  "author": [
    "Anton Stalnuhhin <anton.stalnuhhin@gmail.com>"
  ],
  "private": true,
  "homepage": "https://github.com/devclub/site",
  "repository": {
    "type": "git",
    "url": "devclub/site.git"
  },
  "scripts": {
    "ng": "ng",
    "clean": "rimraf dist",
    "start:eu": "ng serve --aot --open",
    "start:ee": "ng serve --configuration=dev-ee --aot --open",
    "start": "npm run start:eu",
    "build:dev:eu": "npm run clean && ng build --dev",
    "build:dev:ee": "npm run clean && ng build --dev --configuration=dev-ee",
    "build:prod:eu": "npm run clean && ng build --prod --configuration=prod-eu",
    "build:prod:ee": "npm run clean && ng build --prod --configuration=prod-ee",
    "extract": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/*.json --sort --format json",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/common": "7.2.14",
    "@angular/compiler": "7.2.14",
    "@angular/core": "7.2.14",
    "@angular/forms": "7.2.14",
    "@angular/http": "7.2.14",
    "@angular/platform-browser": "7.2.14",
    "@angular/platform-browser-dynamic": "7.2.14",
    "@angular/router": "7.2.14",
    "@fortawesome/fontawesome": "1.1.8",
    "@fortawesome/fontawesome-free-brands": "5.0.13",
    "@fortawesome/fontawesome-free-regular": "5.0.13",
    "@fortawesome/fontawesome-free-solid": "5.0.13",
    "@ngx-translate/core": "11.0.1",
    "bootstrap": "4.3.1",
    "moment": "2.24.0",
    "classlist.js": "1.1.20150312",
    "core-js": "2.6.5",
    "ngx-bootstrap": "4.1.1",
    "rxjs": "6.5.1",
    "web-animations-js": "2.3.1",
    "zone.js": "0.9.0"
  },
  "devDependencies": {
    "@angular/cli": "7.3.8",
    "@angular/compiler-cli": "7.2.14",
    "@angular/language-service": "7.2.14",
    "@angular/tsc-wrapped": "5.0.0-beta.7",
    "@angular-devkit/build-angular": "0.13.8",
    "@biesbjerg/ngx-translate-extract": "2.3.4",
    "@types/node": "11.13.9",
    "codelyzer": "5.0.1",
    "cpr": "3.0.1",
    "rimraf": "2.6.3",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "typescript": "3.2.4"
  }
}
