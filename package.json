{
  "name": "@devclub/site",
  "version": "2.9.0-SNAPSHOT",
  "description": "Devclub site platform: devclub.eu & devclub.ee",
  "author": [
    "Anton Stalnuhhin <anton.stalnuhhin@gmail.com>"
  ],
  "private": true,
  "homepage": "https://github.com/devclub/site",
  "repository": {
    "type": "git",
    "url": "devclub/site.git"
  },
  "scripts": {
    "ng": "ng",
    "clean": "rimraf dist",
    "start:eu": "ng serve --aot --open",
    "start:ee": "ng serve --configuration=dev-ee --aot --open",
    "start": "npm run start:eu",
    "build:dev:eu": "npm run clean && ng build --dev",
    "build:dev:ee": "npm run clean && ng build --dev --configuration=dev-ee",
    "build:prod:eu": "npm run clean && ng build --prod --configuration=prod-eu",
    "build:prod:ee": "npm run clean && ng build --prod --configuration=prod-ee",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "8.2.9",
    "@angular/common": "8.2.9",
    "@angular/compiler": "8.2.9",
    "@angular/core": "8.2.9",
    "@angular/forms": "8.2.9",
    "@angular/platform-browser": "8.2.9",
    "@angular/platform-browser-dynamic": "8.2.9",
    "@angular/router": "8.2.9",
    "@fortawesome/fontawesome-common-types": "0.2.25",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@fortawesome/free-brands-svg-icons": "5.11.2",
    "@fortawesome/angular-fontawesome": "0.5.0",
    "bootstrap": "4.3.1",
    "classlist.js": "1.1.20150312",
    "core-js": "2.6.5",
    "moment": "2.24.0",
    "ngx-bootstrap": "5.1.2",
    "rxjs": "6.5.3",
    "web-animations-js": "2.3.2",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular/cli": "8.3.7",
    "@angular/compiler-cli": "8.2.9",
    "@angular/language-service": "8.2.9",
    "@angular/tsc-wrapped": "5.0.0-beta.7",
    "@angular-devkit/build-angular": "0.803.7",
    "@types/node": "12.7.9",
    "codelyzer": "5.1.2",
    "cpr": "3.0.1",
    "rimraf": "3.0.0",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "typescript": "3.5.3"
  }
}
