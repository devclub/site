{
  "name": "@devclub/site",
  "version": "2.9.0-SNAPSHOT",
  "description": "Devclub site platform: devclub.eu & devclub.ee",
  "author": [
    "Anton Stalnuhhin <anton.stalnuhhin@gmail.com>"
  ],
  "private": true,
  "homepage": "https://github.com/devclub/site",
  "repository": {
    "type": "git",
    "url": "devclub/site.git"
  },
  "scripts": {
    "ng": "ng",
    "clean": "rimraf dist",
    "start:eu": "ng serve --aot --open",
    "start:ee": "ng serve --configuration=dev-ee --aot --open",
    "start": "npm run start:eu",
    "build:dev:eu": "npm run clean && ng build --dev",
    "build:dev:ee": "npm run clean && ng build --dev --configuration=dev-ee",
    "build:prod:eu": "npm run clean && ng build --configuration=prod-eu",
    "build:prod:ee": "npm run clean && ng build --configuration=prod-ee",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "11.0.5",
    "@angular/common": "11.0.5",
    "@angular/compiler": "11.0.5",
    "@angular/core": "11.0.5",
    "@angular/forms": "11.0.5",
    "@angular/platform-browser": "11.0.5",
    "@angular/platform-browser-dynamic": "11.0.5",
    "@angular/router": "11.0.5",
    "@fortawesome/fontawesome-common-types": "0.2.32",
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/free-brands-svg-icons": "5.15.1",
    "@fortawesome/angular-fontawesome": "0.8.1",
    "bootstrap": "4.5.3",
    "classlist.js": "1.1.20150312",
    "core-js": "3.8.1",
    "moment": "2.29.1",
    "ngx-bootstrap": "6.2.0",
    "rxjs": "6.6.3",
    "web-animations-js": "2.3.2",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.5",
    "@angular/cli": "11.0.5",
    "@angular/compiler-cli": "11.0.5",
    "@angular/language-service": "11.0.5",
    "@angular/tsc-wrapped": "5.0.0-beta.7",
    "@types/node": "14.14.19",
    "codelyzer": "6.0.1",
    "cpr": "3.0.1",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.0.5"
  }
}
